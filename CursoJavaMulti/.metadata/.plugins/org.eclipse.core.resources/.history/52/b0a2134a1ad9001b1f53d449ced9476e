package fundamentos;

public class Temperatura {

	public static void main(String[] args) {
		// (°F - 32) x 5/9 = °C
		//   Var const const  var
		
		double fahrenheit = 57;
		final double AJUSTE = 32;
		final double FATOR = 5/9.0;
		double celsius = 0;
		
		celsius = (fahrenheit - AJUSTE) * FATOR;
		System.out.println(celsius);
	}

}

package fundamentos;

public class Temperatura {

	public static void main(String[] args) {
		// (°F - 32) x 5/9 = °C
		//   Var const const  var
		
		double f = 50;
		final double FAT = 32;
		final double DIF = 5/9.0;
		double c = 0;
		
		
		double ffat = f - FAT;
		double ffatDif = ffat * DIF;
		c = ffatDif;
		
		System.out.println(ffat);
		System.out.println(DIF);
		System.out.println(ffatDif);

		
	}

}

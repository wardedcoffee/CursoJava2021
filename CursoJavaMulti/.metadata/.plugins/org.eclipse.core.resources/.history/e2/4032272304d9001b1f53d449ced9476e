package fundamentos;

public class AreaCircunferencia {
	
	public static void main(String[] args) {
	    double raio = 3.4;
	    final double pi = 3.14159; // 'final' torna a vari√°vel constante 
	    
	    //pi = 1;
	    double area = pi * raio * raio;
	    
	    // syso + ctrl + barra
	    System.out.println(area);
	}
}
